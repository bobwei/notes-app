{
  "parser": "babel-eslint",
  "plugins": [],
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "import/no-duplicates": 0,
    "no-use-before-define": 0,
    "prefer-template": 0,
    "consistent-return": 0,
    "no-restricted-properties": 0,
    "react/prop-types": 0,
    "no-useless-return": 0,
    "no-console": 0
  }
}
